  "overrides": {
    "monaco-themes": "0.4.7"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "setup": "./scripts/setup.sh && npm install && npm run rebuild",
    "postsetup": "./scripts/defaultNpmrc.sh",
    "start": "GENERATE_SOURCEMAP=false craco start",
    "build": "npm run setup && npm run prettify && npm run build:css && concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "build:css": "tailwindcss -i src/index.css -o public/tailwind.css -m",
    "build:css:watch": "tailwindcss -i src/index.css -o public/tailwind.css -m --watch",
    "dry": "npm run prettify && npm run bump && concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "dev": "concurrently -k \"npm run build:css:watch\" \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "prod": "NODE_ENV=prod npm run prettify && npm run bump-tag && npm run build",
    "electron": "electron .",
    "package": "craco build && electron-forge package",
    "make": "craco build && electron-forge make --skip-package --inspect",
    "bump": "npm version --no-git-tag-version patch",
    "bump-tag": "npm version -m patch",
    "prettify": "npx prettier --write . ",
    "clean": "npm run cache clean && rm ./npm run.lock",
    "package-widgets": "NODE_ENV=prod rollup -c",
    "package-zip": "npm run package-widgets && node scripts/packageZip.js",
    "publish-to-registry": "node scripts/publishToRegistry.js",
    "rebuild": "electron-rebuild -f -w node-expat",
    "publish-forge": "electron-forge publish -- --from-dry-run",
    "apple-notarize": "./scripts/notarize.sh",
    "apple-history": "./scripts/notarize-history.sh",
    "apple-log": "./scripts/notarize-log.sh",
    "apple-staple": "./scripts/staple.sh",
    "validate:manifest": "node scripts/validateWidget.cjs",
    "test:widgets": "node scripts/testWidgetIntegration.cjs",
    "test:e2e": "npx playwright test --config=e2e/playwright.config.js",
    "test:e2e:headed": "npx playwright test --config=e2e/playwright.config.js --headed",
    "test:all": "npm run test:widgets && npm run test:e2e",
    "validate:full": "./scripts/validate.sh && npm run test:widgets && npm run test:e2e",
    "prdize": "node scripts/prdize.js",
    "test:prd": "node scripts/test-prd.js",
    "prd:coverage": "node scripts/prd-coverage.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false,
    "stream": false,
    "http": false,
    "https": false,
    "zlib": false,
    "url": false,
    "assert": false,
    "util": false,
    "buffer": false,
    "process": false,
    "vm": false,
    "child_process": false,
    "net": false,
    "tls": false,
    "dns": false
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
